{
  "name": "angular-elements-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:elements": "npm run build:dynamic-form-element && npm run build:header-element && npm run build:input-element && npm run build:select-element",
    "build:dynamic-form-element": "npm run bundle:dynamic-form-element && npm run copy:dynamic-form-element",
    "copy:dynamic-form-element": "npm run copy:dynamic-form-element-angular && npm run copy:dynamic-form-element-vanilla && npm run copy:dynamic-form-element-react && npm run copy:dynamic-form-element-vue",
    "copy:dynamic-form-element-angular": "cpr dist/dynamic-form-element/main.js src/assets/elements/dynamic-form-element.bundle.js -o",
    "copy:dynamic-form-element-vanilla": "cpr dist/dynamic-form-element/main.js demos/vanilla-js/elements/dynamic-form-element.bundle.js -o",
    "copy:dynamic-form-element-react": "cpr dist/dynamic-form-element/main.js demos/react-app/public/elements/dynamic-form-element.bundle.js -o",
    "copy:dynamic-form-element-vue": "cpr dist/dynamic-form-element/main.js demos/vue-app/public/elements/dynamic-form-element.bundle.js -o",
    "bundle:dynamic-form-element": "ng build --project dynamic-form-element --prod --output-hashing none",
    "build:header-element": "npm run bundle:header-element && npm run copy:header-element",
    "copy:header-element": "npm run copy:header-element-angular && npm run copy:header-element-vanilla && npm run copy:header-element-react && npm run copy:header-element-vue",
    "copy:header-element-angular": "cpr dist/header-element/main.js src/assets/elements/header-element.bundle.js -o",
    "copy:header-element-vanilla": "cpr dist/header-element/main.js demos/vanilla-js/elements/header-element.bundle.js -o",
    "copy:header-element-react": "cpr dist/header-element/main.js demos/react-app/public/elements/header-element.bundle.js -o",
    "copy:header-element-vue": "cpr dist/header-element/main.js demos/vue-app/public/elements/header-element.bundle.js -o",
    "bundle:header-element": "ng build --project header-element --prod --output-hashing none",
    "build:input-element": "npm run bundle:input-element && npm run copy:input-element",
    "copy:input-element": "npm run copy:input-element-angular && npm run copy:input-element-vanilla && npm run copy:input-element-react && npm run copy:input-element-vue",
    "copy:input-element-angular": "cpr dist/input-element/main.js src/assets/elements/input-element.bundle.js -o",
    "copy:input-element-vanilla": "cpr dist/input-element/main.js demos/vanilla-js/elements/input-element.bundle.js -o",
    "copy:input-element-react": "cpr dist/input-element/main.js demos/react-app/public/elements/input-element.bundle.js -o",
    "copy:input-element-vue": "cpr dist/input-element/main.js demos/vue-app/public/elements/input-element.bundle.js -o",
    "bundle:input-element": "ng build --project input-element --prod --output-hashing none",
    "build:select-element": "npm run bundle:select-element && npm run copy:select-element",
    "copy:select-element": "npm run copy:select-element-angular && npm run copy:select-element-vanilla && npm run copy:select-element-react && npm run copy:select-element-vue",
    "copy:select-element-angular": "cpr dist/select-element/main.js src/assets/elements/select-element.bundle.js -o",
    "copy:select-element-vanilla": "cpr dist/select-element/main.js demos/vanilla-js/elements/select-element.bundle.js -o",
    "copy:select-element-react": "cpr dist/select-element/main.js demos/react-app/public/elements/select-element.bundle.js -o",
    "copy:select-element-vue": "cpr dist/select-element/main.js demos/vue-app/public/elements/select-element.bundle.js -o",
    "bundle:select-element": "ng build --project select-element --prod --output-hashing none",
    "start:vanilla": "lite-server --baseDir='demos/vanilla-js' --config='demos/vanilla-js/server-config.json'",
    "start:react": "cd demos/react-app && npm start",
    "start:vue": "cd demos/vue-app && npm start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.4.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/elements": "^6.0.7",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.4.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@webcomponents/custom-elements": "^1.1.2",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angular-devkit/build-angular": "~0.6.0",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "cpr": "^3.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.4.0",
    "ngx-build-plus": "^1.1.0",
    "prettier": "^1.13.7",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}